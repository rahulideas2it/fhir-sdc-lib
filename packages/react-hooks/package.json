{
  "name": "@rahulideas2it/fhir-sdc-lib-react-hooks",
  "version": "0.0.0",
  "description": "React Hooks Library",
  "keywords": [
    "fhir",
    "healthcare",
    "interoperability",
    "json",
    "serialization",
    "hl7",
    "standards",
    "clinical",
    "dstu2",
    "stu3",
    "r4",
    "normative",
    "ui",
    "input",
    "react",
    "react-component"
  ],
  "license": "Apache-2.0",
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local && cp dist/types.d.ts dist/cjs/index.d.ts && cp dist/types.d.ts dist/esm/index.d.ts",
    "build": "npm run clean && tsc && node esbuild.mjs && npm run api-extractor",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "@medplum/core": "npm:@rahulideas2it/fhir-sdc-lib-core@*",
    "@medplum/definitions": "npm:@rahulideas2it/fhir-sdc-lib-definitions@*",
    "@medplum/fhirtypes": "npm:@rahulideas2it/fhir-sdc-lib-fhirtypes@*",
    "@medplum/mock": "npm:@rahulideas2it/fhir-sdc-lib-mock@*"
  },
  "devDependencies": {
    "@testing-library/dom": "9.3.4",
    "@testing-library/jest-dom": "6.3.0",
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.8",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "jest": "29.7.0",
    "jest-each": "29.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@medplum/core": "*",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
